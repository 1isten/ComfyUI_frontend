{
  "input.2d": {
    "name": "input.2d",
    "category": "input",
    "display_name": "2D",
    "description": "2D input.",
    "python_module": "nodes",
    "input": {
      "required": {
        "source": ["STRING"]
      }
    },
    "input_order": {
      "required": ["source"]
    },
    "output": ["2D"],
    "output_name": ["data"],
    "output_is_list": [false],
    "output_node": false
  },

  "plugin.smooth.smooth_2d": {
    "name": "plugin.smooth.smooth_2d",
    "category": "plugins/smooth",
    "display_name": "Smooth (2D)",
    "description": "main function",
    "python_module": "custom_nodes.smooth.smooth_2d",
    "input": {
      "required": {
        "data": ["2D"]
      },
      "optional": {
        "sigma": [
          "FLOAT",
          {
            "default": 4,
            "min": 0,
            "max": 50,
            "step": 0.1
          }
        ]
      }
    },
    "input_order": {
      "required": ["data"],
      "optional": ["sigma"]
    },
    "output": ["2D", "STRING"],
    "output_name": ["data", "test_txt"],
    "output_is_list": [false, false],
    "output_node": false
  },

  "output.data_to_image.main": {
    "name": "output.data_to_image.main",
    "category": "output/data_to_image",
    "display_name": "Save to Image",
    "description": "Main function.",
    "python_module": "custom_nodes.data_to_image.main",
    "input": {
      "required": {
        "data": ["2D"],
        "path": ["STRING"]
      },
      "optional": {
        "normalize": [
          "BOOLEAN",
          {
            "default": false
          }
        ]
      }
    },
    "input_order": {
      "required": ["data", "path"],
      "optional": ["normalize"]
    },
    "output": [],
    "output_name": [],
    "output_is_list": [],
    "output_node": true
  }
