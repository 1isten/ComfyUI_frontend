{
  "Input.2D": {
    "name": "Input.2D",
    "display_name": "2D",
    "description": "Input 2D data.",
    "python_module": "custom_nodes.PMT Core.Input",
    "category": "input",
    "input": {
      "required": {
        "source": ["STRING"]
      }
    },
    "input_order": {
      "required": ["source"]
    },
    "output": ["2D"],
    "output_name": ["data"],
    "output_is_list": [false],
    "output_node": false
  },

  "PluginFunction.smooth.smooth_2d": {
    "name": "PluginFunction.smooth.smooth_2d",
    "display_name": "smooth_2d",
    "description": "Smooth 2D data.",
    "python_module": "custom_nodes.PMT Core.Plugin Function",
    "category": "plugins/smooth",
    "input": {
      "required": {
        "data": ["2D"],
        "sigma": [
          "FLOAT",
          {
            "default": 4,
            "min": 0,
            "max": 50,
            "step": 0.1,
            "round": 0.01
          }
        ]
      }
    },
    "input_order": {
      "required": ["data", "sigma"]
    },
    "output": ["2D", "STRING"],
    "output_name": ["data", "test_txt"],
    "output_is_list": [false, false],
    "output_node": false
  },

  "Output.data_to_image.main": {
    "name": "Output.data_to_image.main",
    "display_name": "main",
    "description": "Save 2D numpy.ndarray to image file.",
    "python_module": "custom_nodes.PMT Core.Output",
    "category": "output/data_to_image",
    "input": {
      "required": {
        "data": ["2D"],
        "path": ["STRING"],
        "normalize": [
          "BOOLEAN",
          {
            "default": false
          }
        ]
      }
    },
    "input_order": {
      "required": ["data", "path", "normalize"]
    },
    "output": [],
    "output_name": [],
    "output_is_list": [],
    "output_node": true
  },

  "Preview.data_to_image.main": {
    "name": "Preview.data_to_image.main",
    "display_name": "main",
    "description": "Preview the 2D numpy.ndarray image.",
    "python_module": "custom_nodes.PMT Core.Preview",
    "category": "preview/data_to_image",
    "input": {
      "required": {
        "data": ["2D"],
        "path": ["STRING"],
        "normalize": [
          "BOOLEAN",
          {
            "default": false
          }
        ]
      },
      "hidden": {
        "prompt": "PROMPT",
        "extra_pnginfo": "EXTRA_PNGINFO"
      }
    },
    "input_order": {
      "required": ["data", "path", "normalize"],
      "hidden": ["prompt", "extra_pnginfo"]
    },
    "output": [],
    "output_name": [],
    "output_is_list": [],
    "output_node": true
  }
}
