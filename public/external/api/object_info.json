  "rag_llm.text_splitter": {
    "name": "rag_llm.text_splitter",
    "category": "LLM",
    "display_name": "Text Splitter",
    "description": "Split text.",
    "python_module": "custom_nodes.rag_llm.text_splitter",
    "input": {
      "required": {
        "text": [
          "STRING",
          {
            "forceInput": true
          }
        ],
        "type": [
