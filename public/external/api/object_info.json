            "default": 350,
            "min": 1
          }
        ],
        "chunk_overlap": [
          "INT",
          {
            "default": 0,
            "min": 0
          }
        ]
      },
      "optional": {
        "separators": [
          "STRING",
          {
            "multiline": false
          }
        ]
      }
    },
    "input_order": {
      "required": ["text", "type", "chunk_size", "chunk_overlap"],
      "optional": ["separators"]
    },
    "output": ["STRING"],
    "output_name": ["text"],
    "output_is_list": [false],
    "output_node": false
  },
